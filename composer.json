{
    "name": "ridhwan/laravel-bank-bri",
    "description": "Laravel Package for Bank Bri Api",
    "type": "library",
    "extra": {
        "laravel": {
            "providers": [
                "Ridhwan\\LaravelBankBri\\Providers\\BriServiceProvider"
            ],
            "aliases": {
                "BcaAPI": "Ridhwan\\LaravelBankBri\\Facades\\BriFacade"
            }
        }
    },
    "require": {
        "php": ">=7.2|^8.0",
        "ridhwan/response": "^1.0|dev-master",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ridhwan\\LaravelBankBri\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "ridhwan",
            "email": "ridhwanmuhammad772@gmail.com"
        }
    ],
    "minimum-stability": "dev"
